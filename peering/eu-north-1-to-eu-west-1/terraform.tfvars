# Global tags for resources
global_tags = {
  Environment = "Development"
  ManagedBy   = "Terraform"
}

# Set requester region name to "Stockholm".
requester_region_name = "Stockholm"

# Set accepter region name to "Ireland".
accepter_region_name = "Ireland"

# Define the AWS region for peering as "eu-west-1".
peer_region = "eu-west-1"

# Specify the AWS region for the requester as "eu-north-1".
requester_region = "eu-north-1"
