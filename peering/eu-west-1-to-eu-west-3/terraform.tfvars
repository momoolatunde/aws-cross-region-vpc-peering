# Global tags for resources
global_tags = {
  Environment = "Development"
  ManagedBy   = "Terraform"
}

# Set requester region name to "Ireland".
requester_region_name = "Ireland"

# Set accepter region name to "Paris".
accepter_region_name = "Paris"

# Define the AWS region for peering as "eu-west-3".
peer_region = "eu-west-3"

# Specify the AWS region for the requester as "eu-west-1".
requester_region = "eu-west-1"
