# Global tags for resources
global_tags = {
  Environment = "Development"
  ManagedBy   = "Terraform"
}

# Set requester region name to "Paris".
requester_region_name = "Paris"

# Set accepter region name to "Stockholm".
accepter_region_name = "Stockholm"

# Define the AWS region for peering as "eu-north-1".
peer_region = "eu-north-1"

# Specify the AWS region for the requester as "eu-west-3".
requester_region = "eu-west-3"
